    "lint-staged": "15.2.5",
    "localforage": "1.10.0",
    "match-sorter": "6.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.23.1",
    "sort-by": "1.2.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "5.35.6",
    "@types/node": "20.12.12",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.2.0",
