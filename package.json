  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "VITE_API_BASE_URL=http://localhost:5000 vite",
    "dev:serverless": "VITE_API_BASE_URL=https://yojnqzou3f.execute-api.ap-southeast-2.amazonaws.com vite",
